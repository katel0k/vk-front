{
    "presets": [
        "@babel/preset-env",
        [
            "@babel/preset-react",
            {
                "runtime": "automatic"
            }
        ],
        "@babel/preset-typescript"
    ],
    "plugins": [
        [
            "@dr.pogodin/react-css-modules", {
                "generateScopedName": "[path]__[name]__[local]__[hash:base64:5]",
                "webpackHotModuleReloading": true
            }
        ]
    ]
}
